            <column name="linear_id" type="NVARCHAR(64)"/>
            <column name="product_order_did" type="NVARCHAR(64)"/>
            <column name="service_level_agreement_did" type="NVARCHAR(64)"/>
        </createTable>
    </changeSet>
</databaseChangeLog>